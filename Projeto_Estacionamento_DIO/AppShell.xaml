<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Projeto_Estacionamento_DIO.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Projeto_Estacionamento_DIO.Pages"
    Title="Projeto_Estacionamento_DIO"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        ContentTemplate="{DataTemplate local:Init_Page}"
        Route="Init_Page" />

    <ShellContent
     ContentTemplate="{DataTemplate local:Cadastro_Page}"
     Route="Cadastro_Page" />

    <ShellContent
     ContentTemplate="{DataTemplate local:Historico_Page}"
     Route="Historico_Page" />

</Shell>
